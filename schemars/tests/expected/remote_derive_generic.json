{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "MyStruct_for_int32",
  "type": "object",
  "required": [
    "byte_or_bool2",
    "fake_map",
    "s",
    "unit_or_t2"
  ],
  "properties": {
    "byte_or_bool2": {
      "$ref": "#/components/schemas/Or_for_uint8_and_Boolean"
    },
    "unit_or_t2": {
      "$ref": "#/components/schemas/Or_for_Null_and_int32"
    },
    "s": {
      "$ref": "#/components/schemas/Str"
    },
    "fake_map": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "prefixItems": [
          {
            "type": "string"
          }
        ],
        "uniqueItems": true
      }
    }
  },
  "definitions": {
    "Or_for_uint8_and_Boolean": {
      "anyOf": [
        {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        },
        {
          "type": "boolean"
        }
      ]
    },
    "Or_for_Null_and_int32": {
      "anyOf": [
        {
          "type": "null"
        },
        {
          "type": "integer",
          "format": "int32"
        }
      ]
    },
    "Str": {
      "type": "string"
    }
  }
}